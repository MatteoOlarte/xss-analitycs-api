  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI (uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["src.main:app", "--reload"],
      "jinja": true
    },
    {
      "name": "Python Debugger: FastAPI (fastapi CLI)",
      "type": "debugpy",
      "request": "launch",
      "module": "fastapi",
      "args": ["dev", "src/main.py"],
      "jinja": true
    }
  ]
}
